   // Window Border menu icons //
   #define BORDERICON(NAME, FILE)                     \
   group {                                            \
      name: "e/widgets/border/default/"NAME ;         \
      max: 64 64;                                     \
      parts {                                         \
         part {                                       \
            name: "icon";                             \
            mouse_events: 1;                          \
            description {                             \
               state: "default" 0.0;                  \
               aspect: 1.0 1.0;                       \
               aspect_preference: BOTH;               \
               image.image: FILE COMP;                \
               image.normal: FILE ;                   \
            }                                         \
         }                                            \
      }                                               \
   }

   BORDERICON("basic","icon_border_more.png")
