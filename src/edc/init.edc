group {
      name: "e/init/splash";
      /* images used */
      images {
         image: "exq-bglight.png" COMP;
         image: "moksha_logo.png" COMP;
         image: "exq-dot.png" COMP;
         image: "exq-dot-glow.png" COMP;
      }
      script {
         public is_ready;
         public do_end;
      }
      
